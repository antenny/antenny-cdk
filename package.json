{
  "name": "antenny-cdk",
  "version": "2.0.0",
  "description": "cdk constructs for antenny",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint .",
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "compile": "tsc",
    "test": "tsc --build tsconfig.json && jest --runInBand"
  },
  "author": "Jacob Breindel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antenny/antenny-cdk.git"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "antenny-cdk",
        "module": "antenny_cdk"
      },
      "dotnet": {
        "namespace": "Antenny.Cdk",
        "packageId": "Antenny.Cdk"
      }
    }
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "^1.80.0",
    "@aws-cdk/aws-lambda": "^1.80.0",
    "@aws-cdk/aws-logs": "^1.80.0",
    "@aws-cdk/core": "^1.80.0",
    "@aws-cdk/custom-resources": "^1.80.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "jsii": "^1.16.0",
    "jsii-pacmak": "^1.16.0",
    "ts-jest": "^26.4.4"
  },
  "peerDependencies": {
    "@aws-cdk/aws-iam": "^1.80.0",
    "@aws-cdk/aws-lambda": "^1.80.0",
    "@aws-cdk/aws-logs": "^1.80.0",
    "@aws-cdk/core": "^1.80.0",
    "@aws-cdk/custom-resources": "^1.80.0"
  }
}

{
  "name": "antenny-cdk",
  "version": "1.0.0",
  "description": "cdk constructs for antenny",
  "main": "lib/index.js",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "test": "tsc && jest",
    "watch": "tsc -w"
  },
  "author": "Jacob Breindel",
  "license": "MIT",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "antenny-cdk.constructs",
        "module": "antenny_cdk.constructs"
      },
      "java": {
        "package": "io.antenny.cdk.constructs",
        "maven": {
          "groupId": "io.antenny.cdk",
          "artifactId": "constructs"
        }
      },
      "dotnet": {
        "namespace": "Antenny.Cdk.Constructs",
        "packageId": "Antenny.Cdk.Constructs"
      }
    }
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "^1.80.0",
    "@aws-cdk/aws-lambda": "^1.80.0",
    "@aws-cdk/aws-logs": "^1.80.0",
    "@aws-cdk/core": "^1.80.0",
    "@aws-cdk/custom-resources": "^1.80.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "jest": "^26.6.3",
    "jsii": "^1.16.0",
    "jsii-pacmak": "^1.16.0",
    "ts-jest": "^26.4.4"
  }
}
